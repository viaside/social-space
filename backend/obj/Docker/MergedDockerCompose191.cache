name: dockercompose10004229053971030517
services:
  backend:
    build:
      context: D:\Проекты\social-space\backend
      dockerfile: Backend/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Backend
      target: base
    container_name: Backend
    depends_on:
      postgres:
        condition: service_started
    entrypoint:
    - tail
    - -f
    - /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      DOTNET_USE_POLLING_FILE_WATCHER: "1"
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: backend:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/bin/Debug/net6.0/Backend.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      backend-dev: null
    ports:
    - mode: ingress
      target: 80
      published: "5000"
      protocol: tcp
    tty: true
    volumes:
    - type: bind
      source: D:\Проекты\social-space\backend\Backend
      target: /app
      bind:
        create_host_path: true
    - type: bind
      source: D:\Проекты\social-space\backend
      target: /src
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\vanek\vsdbg\vs2017u5
      target: /remote_debugger
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\vanek\.nuget\packages\
      target: /root/.nuget/packages
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Program Files (x86)\Microsoft Visual Studio\Shared\NuGetPackages
      target: /root/.nuget/fallbackpackages
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\vanek\AppData\Roaming/ASP.NET/Https
      target: /root/.aspnet/https
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\vanek\AppData\Roaming/Microsoft/UserSecrets
      target: /root/.microsoft/usersecrets
      read_only: true
      bind:
        create_host_path: true
  postgres:
    environment:
      POSTGRES_DB: OpenSpaceDB
      POSTGRES_PASSWORD: "123"
      POSTGRES_USER: postgres
    image: postgres:latest
    networks:
      backend-dev: null
    ports:
    - mode: ingress
      target: 5432
      protocol: tcp
    restart: always
networks:
  backend-dev:
    name: dockercompose10004229053971030517_backend-dev
    driver: bridge
name: dockercompose10004229053971030517
services:
  backend:
    build:
      context: D:\Проекты\social-space\backend
      dockerfile: Backend/Dockerfile
    container_name: backend_container
    environment:
      ASPNETCORE_ENVIRONMENT: Development
    image: backend
    networks:
      default: null
    ports:
    - mode: ingress
      target: 80
      published: "5000"
      protocol: tcp
  pgadmin:
    container_name: pgadmin_container
    deploy:
      resources:
        limits:
          cpus: "0.5"
          memory: "1073741824"
    environment:
      PGADMIN_CONFIG_SERVER_MODE: "False"
      PGADMIN_DEFAULT_EMAIL: habrpguser@habr.com
      PGADMIN_DEFAULT_PASSWORD: pgadminpwd4habr
    image: dpage/pgadmin4:5.7
    networks:
      postgres: null
    ports:
    - mode: ingress
      target: 80
      published: "5050"
      protocol: tcp
    restart: unless-stopped
    volumes:
    - type: bind
      source: D:\Проекты\social-space\backend\pgadmin
      target: /var/lib/pgadmin
      bind:
        create_host_path: true
  postgres:
    container_name: postgres_container
    environment:
      POSTGRES_DB: OSdb
      POSTGRES_HOST_AUTH_METHOD: trust
      POSTGRES_PASSWORD: OS
      POSTGRES_USER: postgres
    image: postgres:latest
    networks:
      default: null
    ports:
    - mode: ingress
      target: 5432
      published: "5432"
      protocol: tcp
    restart: always
networks:
  default:
    name: dockercompose10004229053971030517_default
  postgres:
    name: dockercompose10004229053971030517_postgres
    driver: bridge
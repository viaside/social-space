43F\\u0440\\u0430\\u0432\\u0438\\u043B\\u043E, \\u043E\\u043D\\u0430 \\u0432\\u043A\\u043B\\u044E\\u0447\\u0435\\u043D\\u0430 \\u043F\\u043E \\u0443\\u043C\\u043E\\u043B\\u0447\\u0430\\u043D\\u0438\\u044E).\"}),/*#__PURE__*/_jsx(\"li\",{children:\"\\u0437\\u0430\\u0439\\u0434\\u0438\\u0442\\u0435 \\u0432 \\u043D\\u0430\\u0441\\u0442\\u0440\\u043E\\u0439\\u043A\\u0438 \\u043F\\u0440\\u0438\\u0432\\u0430\\u0442\\u043D\\u043E\\u0441\\u0442\\u0438 (Group Privacy) \\u0438 \\u043E\\u0442\\u043A\\u043B\\u044E\\u0447\\u0438\\u0442\\u0435 \\u0440\\u0435\\u0436\\u0438\\u043C \\u043A\\u043E\\u043D\\u0444\\u0438\\u0434\\u0435\\u043D\\u0446\\u0438\\u0430\\u043B\\u044C\\u043D\\u043E\\u0441\\u0442\\u0438 \\u0431\\u043E\\u0442\\u0430\"})]}),/*#__PURE__*/_jsx(\"br\",{}),/*#__PURE__*/_jsxs(\"p\",{children:[\"\\u0414\\u043B\\u044F \",/*#__PURE__*/_jsx(\"b\",{children:\"\\u0441\\u0431\\u043E\\u0440\\u0430 \\u0441\\u043E\\u043E\\u0431\\u0449\\u0435\\u043D\\u0438\\u0439\"}),\"  \\u0438\\u0437 \\u0433\\u0440\\u0443\\u043F\\u043F\\u043E\\u0432\\u043E\\u0433\\u043E \\u0447\\u0430\\u0442\\u0430 \\u0434\\u043E\\u0431\\u0430\\u0432\\u044C\\u0442\\u0435 \\u0431\\u043E\\u0442\\u0430 \\u0432 \\u0447\\u0430\\u0442:\"]}),/*#__PURE__*/_jsxs(\"ul\",{children:[/*#__PURE__*/_jsx(\"li\",{children:\"\\u0414\\u0430\\u043B\\u0435\\u0435 \\u043D\\u0435\\u043E\\u0431\\u0445\\u043E\\u0434\\u0438\\u043C\\u043E \\u0434\\u0430\\u0442\\u044C \\u0435\\u043C\\u0443 \\u043F\\u0440\\u0430\\u0432\\u0430 \\u0430\\u0434\\u043C\\u0438\\u043D\\u0438\\u0441\\u0442\\u0440\\u0430\\u0442\\u043E\\u0440\\u0430. \\u042D\\u0442\\u043E \\u043C\\u043E\\u0436\\u043D\\u043E \\u0441\\u0434\\u0435\\u043B\\u0430\\u0442\\u044C \\u0432 \\u0440\\u0430\\u0437\\u0434\\u0435\\u043B\\u0435 \\u041D\\u0430\\u0441\\u0442\\u0440\\u043E\\u0439\\u043A\\u0438 - \\u0423\\u043F\\u0440\\u0430\\u0432\\u043B\\u0435\\u043D\\u0438\\u0435 \\u0433\\u0440\\u0443\\u043F\\u043F\\u043E\\u0439 - \\u0410\\u0434\\u043C\\u0438\\u043D\\u0438\\u0441\\u0442\\u0440\\u0430\\u0442\\u043E\\u0440\\u044B\"}),/*#__PURE__*/_jsx(\"li\",{children:\"\\u0423\\u0431\\u0435\\u0434\\u0438\\u0442\\u0435\\u0441\\u044C, \\u0447\\u0442\\u043E \\u0443 \\u0431\\u043E\\u0442\\u0430 \\u0435\\u0441\\u0442\\u044C \\u043F\\u0440\\u0430\\u0432\\u0430 \\u043D\\u0430 \\u0443\\u0434\\u0430\\u043B\\u0435\\u043D\\u0438\\u0435 \\u0441\\u043E\\u043E\\u0431\\u0449\\u0435\\u043D\\u0438\\u0439 \\u0438 \\u0431\\u043B\\u043E\\u043A\\u0438\\u0440\\u043E\\u0432\\u043A\\u0443 \\u0443\\u0447\\u0430\\u0441\\u0442\\u043D\\u0438\\u043A\\u043E\\u0432.\"})]})]}),/*#__PURE__*/_jsx(\"div\",{className:\"blockImage\",children:/*#__PURE__*/_jsx(\"img\",{src:preview2,alt:\"\",height:350})})]})})]});}","map":{"version":3,"names":["preview1","preview2","preview3","Main"],"sources":["D:/Проекты/social-space/frontend/src/pages/Main.jsx"],"sourcesContent":["// import {preview} from \"../illustration/preview.jsx\";\r\nimport preview1 from \"../illustration/preview1.svg\";\r\nimport preview2 from \"../illustration/preview2.svg\";\r\nimport preview3 from \"../illustration/preview3.svg\";\r\n\r\nexport default function Main() {\r\n    return(\r\n        <>\r\n        <div className=\"head\">\r\n            <h1>Social Space</h1>\r\n            <p>CRM для технической поддержки с интеграции telegram bot api</p>\r\n        </div>\r\n        <div className=\"block blue\">\r\n            <div className=\"blockText\">\r\n                <h1>Зачем это?</h1>\r\n                <p>\r\n                Если у вас есть несколько сообществ в социальных сетях с активной аудиторией, вы наверняка сталкивались с трудностями при реагировании на комментарии к постам сообществ и личные сообщения.\r\n                <br/>\r\n                <br/>\r\n                Social Space разработан специально, чтобы упростить процесс реагирования в собственных сообществах.\r\n                <br/>\r\n                <br/>\r\n                Объединяя потоки из нескольких социальных сетей, Social Space предоставляет единую очередь запросов от клиентов для обработки. Это поможет быстрее отвечать на важные комментарии и сообщения. Обновление данных происходит в режиме онлайн, поэтому вы ничего не пропустите.\r\n                <br />\r\n                <br />\r\n                Social Space – это CRM для соцсетей, которая сама собирает доступную информацию о клиенте. Отвечая клиенту, вы будете видеть всю историю взаимодействия с ним в ваших сообществах. Используйте эти обогащенные данные для полного погружения в решаемый вопрос.\r\n                <br />\r\n                <br />\r\n                Подключите сообщества и пригласите коллег в Social Space, чтобы вместе обрабатывать комментарии и сообщения клиентов.\r\n                </p>\r\n            </div>\r\n        </div>\r\n        <div className=\"block white\">\r\n            <h1>Как это работает?</h1>\r\n            <div className=\"imageText\">\r\n                <img src={preview1} alt=\"\" height={250}/>\r\n                <div className=\"blockText\">\r\n                    <p>\r\n                        После подключения ваших ботов в сервис начнут собираться все комментарии и личные сообщения \r\n                        <br />\r\n                        <br />\r\n                        из подключенных ботов. Вы сможете реагировать на комментарии, сообщения в едином пространстве. \r\n                        <br />\r\n                        <br />\r\n                        Мы взаимодействуем с социальными сетями через официальное API. Задержка сбора комментариев составляет в среднем 10 секунд.\r\n                    </p>\r\n                </div>\r\n            </div>\r\n            <div className=\"leftImage\">\r\n                <img src={preview3} alt=\"\" height={250}/>\r\n            </div>\r\n        </div>\r\n        <div className=\"block blue\">\r\n            <div className=\"imageText\">\r\n                <div className=\"blockText\">\r\n                    <h1 className=\"start\">Как настроить бота?</h1>\r\n                    <p>Для сбора сообщений, отправленных боту напрямую, перейдите на страницу Профиль - Боты, нажмите кнопку “Добавить бота”.</p>\r\n                    <br />\r\n                    <p>В открывшемся окне нужно ввести токен доступа.</p>\r\n                    <br />\r\n                    <p>Для того, чтобы узнать <b>токен</b>  существующего бота, вам необходимо:</p>\r\n                    <ul>\r\n                        <li>открыть Telegram</li>\r\n                        <li>найти в поиске BotFather</li>\r\n                        <li>написать боту команду /mybots</li>\r\n                        <li>выбрать нужного бота</li>\r\n                        <li>выбрать API Token</li>\r\n                    </ul>\r\n                    <br />\r\n                    <p>Для <b>создания нового бота</b>  необходимо:</p>\r\n                    <ul>\r\n                        <li>открыть Telegram</li>\r\n                        <li>найти в поиске BotFather</li>\r\n                        <li>написать боту команду /start</li>\r\n                        <li>написать боту команду /newbot</li>\r\n                        <li>следуя инструкции, завершить создание</li>\r\n                    </ul>   \r\n                    <br />\r\n                    <p>Для того, чтобы сообщения из групповых чатов и нативные комментарии <b>попадали в Social Space</b> , необходимо предварительно произвести дополнительные настройки бота:</p>\r\n                    <ul>\r\n                        <li>в Telegram найдите BotFather</li>\r\n                        <li>если бот не создан, создайте его, следуя инструкции выше</li>\r\n                        <li>если бот уже существует, напишите команду /mybots и выберите нужного</li>\r\n                        <li>зайдите в настройки бота (Bot Settings)</li>\r\n                        <li>проверьте, чтобы возможность добавления бота в группы (Allow Groups) была включена (как правило, она включена по умолчанию).</li>\r\n                        <li>зайдите в настройки приватности (Group Privacy) и отключите режим конфиденциальности бота</li>\r\n                    </ul>\r\n                    <br />\r\n                    <p>Для <b>сбора сообщений</b>  из группового чата добавьте бота в чат:</p>\r\n                    <ul>\r\n                        <li>Далее необходимо дать ему права администратора. Это можно сделать в разделе Настройки - Управление группой - Администраторы</li>\r\n                        <li>Убедитесь, что у бота есть права на удаление сообщений и блокировку участников.</li>\r\n                    </ul>\r\n                </div>\r\n                <div className=\"blockImage\">\r\n                    <img src={preview2} alt=\"\" height={350}/>\r\n                </div>\r\n            </div>\r\n        </div>\r\n        </>\r\n    )\r\n}"],"mappings":"AAAA;AACA,MAAOA,SAAQ,KAAM,8BAA8B,CACnD,MAAOC,SAAQ,KAAM,8BAA8B,CACnD,MAAOC,SAAQ,KAAM,8BAA8B,CAAC,6IAEpD,cAAe,SAASC,KAAI,EAAG,CAC3B,mBACI,wCACA,aAAK,SAAS,CAAC,MAAM,wBACjB,oBAAI,cAAY,EAAK,cACrB,mBAAG,uOAA2D,EAAI,GAChE,cACN,YAAK,SAAS,CAAC,YAAY,uBACvB,aAAK,SAAS,CAAC,WAAW,wBACtB,oBAAI,oDAAU,EAAK,cACnB,qBAAG,89BAEH,2BAAK,cACL,aAAK,keAEL,2BAAK,cACL,aAAK,k1CAEL,2BAAM,cACN,aAAM,0uCAEN,2BAAM,cACN,aAAM,0jBAEN,GAAI,GACF,EACJ,cACN,aAAK,SAAS,CAAC,aAAa,wBACxB,oBAAI,yFAAiB,EAAK,cAC1B,aAAK,SAAS,CAAC,WAAW,wBACtB,YAAK,GAAG,CAAEH,QAAS,CAAC,GAAG,CAAC,EAAE,CAAC,MAAM,CAAE,GAAI,EAAE,cACzC,YAAK,SAAS,CAAC,WAAW,uBACtB,qBAAG,weAEC,2BAAM,cACN,aAAM,ifAEN,2BAAM,cACN,aAAM,inBAEV,GAAI,EACF,GACJ,cACN,YAAK,SAAS,CAAC,WAAW,uBACtB,YAAK,GAAG,CAAEE,QAAS,CAAC,GAAG,CAAC,EAAE,CAAC,MAAM,CAAE,GAAI,EAAE,EACvC,GACJ,cACN,YAAK,SAAS,CAAC,YAAY,uBACvB,aAAK,SAAS,CAAC,WAAW,wBACtB,aAAK,SAAS,CAAC,WAAW,wBACtB,WAAI,SAAS,CAAC,OAAO,UAAC,qGAAmB,EAAK,cAC9C,mBAAG,kmBAAsH,EAAI,cAC7H,aAAM,cACN,mBAAG,mPAA8C,EAAI,cACrD,aAAM,cACN,qBAAG,mHAAuB,iCAAG,gCAAK,EAAI,8LAAqC,GAAI,cAC/E,mCACI,oBAAI,qDAAgB,EAAK,cACzB,oBAAI,sFAAwB,EAAK,cACjC,oBAAI,8HAA6B,EAAK,cACtC,oBAAI,gHAAoB,EAAK,cAC7B,oBAAI,sDAAiB,EAAK,GACzB,cACL,aA